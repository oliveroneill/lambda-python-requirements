    "format": "prettier --write index.js lib/*.js"
  },
  "devDependencies": {
    "eslint": "*",
    "prettier": "*"
  },
  "dependencies": {
    "argparse": "1.0.10",
    "bluebird": "^3.0.6",
    "fs-extra": "^6.0.0",
    "glob-all": "^3.1.0",
    "is-wsl": "^1.1.0",
    "jszip": "^3.1.0",
    "lodash.get": "^4.4.2",
    "lodash.set": "^4.3.2",
    "lodash.uniqby": "^4.0.0",
    "lodash.values": "^4.3.0",
    "rimraf": "^2.6.2"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "env": {
      "node": true,
      "es6": true
    }
  },
  "prettier": {
    "semi": true,
    "singleQuote": true
  }
