{
  "name": "pal",
  "version": "4.0.3",
  "engines": {
    "node": ">=6.0"
  },
  "description": "Python AWS Lambda packager",
  "author": "United Income <engineering@unitedincome.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/oliveroneill/pal"
  },
  "files": [
    "main.js",
    "lib/*.js",
    "LICENSE",
    "package.json",
    "unzip_requirements.py",
    "README.md"
  ],
  "main": "index.js",
  "bin": {
